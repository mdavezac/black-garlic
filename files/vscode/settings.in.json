{
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "FuraCode Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "debug.toolBarLocation": "docked",
  "breadcrumbs.enabled": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.scrollback": 100000,
  "search.location": "panel",
  "vim.statusBarColorControl": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "cmake.configureOnOpen": true,
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "python.unitTest.pyTestEnabled": true,
  "gitlens.advanced.telemetry.enabled": false,
  "editor.wrappingIndent": "deepIndent",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.useExperimentalFileWatcher": true,
  "gitlens.defaultGravatarsStyle": "retro",
  "window.zoomLevel": 1,
  "gitlens.codeLens.enabled": false,
  "catch2TestExplorer.debugConfigurationTemplate": {
    "type": "cppdbg",
    "program": "${exec}",
    "args": "${args}",
    "cwd": "${cwd}",
    "env": "${envObj}",
    "stopAtEntry": false,
    "externalConsole": false,
    "MIMode": "lldb"
  },
  "omnisharp.loggingLevel": "debug",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "editor.wordWrapColumn": 120,
  "csharp.suppressHiddenDiagnostics": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.statusBar.feedback.visible": false,
  "workbench.activityBar.visible": false,
  "vim.easymotion": true,
  "workbench.colorTheme": "Default High Contrast",
  "editor.fontSize": 11,
  "workbench.editor.enablePreview": false,
  "terminal.integrated.fontSize": 11,
  "window.nativeFullScreen": true,
{%- if "toggl" in secrets %}
  "toggl.apiKey": "{{secrets['toggl']['api']}}",
  "toggl.defaultProjectId": {{secrets['toggl']['project']}},
{% endif -%}
}
